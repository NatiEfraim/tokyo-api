    "paths": {
        "/api/inventories": {
            "get": {
                "tags": [
                    "Inventory"
                ],
                "summary": "Get all inventories",
                "description": "Returns a list of all inventories.",
                "operationId": "14778f5d86f3e51cab479d0cf398aac1",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "quantity": {
                                                "type": "integer",
                                                "example": 33
                                            },
                                            "sku": {
                                                "type": "string",
                                                "example": "0028221469208"
                                            },
                                            "item_type": {
                                                "type": "string",
                                                "example": "autem"
                                            },
                                            "detailed_description": {
                                                "type": "string",
                                                "example": "Neque recusandae corporis totam facere pariatur. Et perspiciatis aut in quia. Placeat quas vero modi magni ut. Voluptas et qui vitae culpa."
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "התרחש בעיית שרת יש לנסות שוב מאוחר יותר."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Inventories"
                ],
                "summary": "Create a new inventory item",
                "description": "Store a newly created inventory item in the database.",
                "operationId": "104375b37edb2c1ab9fc350db904eec6",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "quantity",
                                    "sku",
                                    "item_type",
                                    "detailed_description"
                                ],
                                "properties": {
                                    "quantity": {
                                        "type": "integer",
                                        "example": 10
                                    },
                                    "sku": {
                                        "type": "string",
                                        "example": "ABC123"
                                    },
                                    "item_type": {
                                        "type": "string",
                                        "example": "Product"
                                    },
                                    "detailed_description": {
                                        "type": "string",
                                        "example": "Detailed description of the item."
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Success response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Inventory item created successfully."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The given data was invalid."
                                        },
                                        "errors": {
                                            "type": "object",
                                            "example": {
                                                "quantity": [
                                                    "The quantity field is required."
                                                ]
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Internal server error."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/inventories/{id}": {
            "get": {
                "tags": [
                    "Inventory"
                ],
                "summary": "Get inventory record by ID",
                "description": "Returns a single inventory record based on the provided ID.",
                "operationId": "f6225357f13ad4bced159060d6496ed5",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the inventory record",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": 2
                                        },
                                        "quantity": {
                                            "type": "integer",
                                            "example": 43
                                        },
                                        "sku": {
                                            "type": "string",
                                            "example": "2216255278905"
                                        },
                                        "item_type": {
                                            "type": "string",
                                            "example": "quia"
                                        },
                                        "detailed_description": {
                                            "type": "string",
                                            "example": "Vel sunt odit quam qui ut suscipit quo. Ipsum dignissimos totam in totam. Veniam voluptas vitae et repellendus dolores consectetur tempora. Placeat atque provident enim sint et qui."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "יש לשלוח מספר מזה של שורה"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "התרחש בעיית שרת יש לנסות שוב מאוחר יותר."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "Inventories"
                ],
                "summary": "Delete an inventory by ID",
                "description": "Deletes an inventory based on the provided ID.",
                "operationId": "8a703f374100d421322c0852476efffe",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the inventory to delete",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "שורה נמחקה בהצלחה."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "יש לשלוח מספר מזהה של שורה"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not found response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "שורה אינה קיימת במערכת."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Error response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "התרחש בעיית שרת יש לנסות שוב מאוחר יותר."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
